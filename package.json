{
  "name": "react-beginner-template",
  "version": "0.1.0",
  "description": "A basic scaffolding for a friend who's learning web dev",
  "private": true,
  "engines": {
    "node": ">=9"
  },
  "scripts": {
    "clean": "rimraf build && mkdirp build",
    "lint": "eslint src --fix",
    "flow": "flow 2>&1",
    "stylelint": "stylelint \"src/scss/**/*.scss\" --syntax scss --fix",
    "validate": "run-p --aggregate-output -c clean lint flow stylelint",
    "dev": "run-s validate build:dev",
    "dev:serve": "run-s validate serve",
    "build": "webpack -p --color --display-error-details --mode production",
    "build:dev": "webpack --color --display-error-details --display detailed --debug --mode development",
    "serve": "webpack-dev-server --config ./webpack.config.js --mode development --debug -w",
    "test": "jest -c test/jest-config.json 2>&1",
    "test:dev": "jest -c test/jest-config.dev.json 2>&1",
    "test:debug": "node $NODE_DEBUG_OPTION node_modules/.bin/jest --runInBand --no-cache -c test/jest-config.dev.json 2>&1",
    "flow-typed": "flow-typed",
    "flow-typed:update": "npm run flow-typed -- update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PMLavigne/react-beginner-template.git"
  },
  "author": "Patrick Lavigne <me@patrick.codes> (https://github.com/PMLavigne)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/PMLavigne/react-beginner-template/issues"
  },
  "homepage": "https://github.com/PMLavigne/react-beginner-template#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-types": "^6.26.0",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flow": "^2.29.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-scope": "^3.7.1",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.68.0",
    "flow-typed": "^2.4.0",
    "html-webpack-plugin": "^3.0.7",
    "image-webpack-loader": "^4.2.0",
    "jest": "^22.4.3",
    "mini-css-extract-plugin": "^0.2.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "prettier-eslint": "^8.8.1",
    "react-a11y": "^1.0.0",
    "react-docgen": "^2.20.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^2.5.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.5",
    "debug": "^3.1.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "lodash-decorators": "^5.0.0",
    "moment": "^2.21.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-act-async": "^1.7.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  }
}
